if ($("#note_<%= @note.id %>_tags").is(':empty')) {
  
    $("#tags_<%= @note.id %>").hide();
    $("#note_<%= @note.id %>_tags").html("<%= j render "notes/tag"  %>")

    $("#tag_list_<%= @note.id %>").tokenInput( '/tags.json',{
      theme: 'facebook',
    prePopulate: $("#tag_list_<%= @note.id %>").data('load')
    });
}
else {
  $("#note_<%= @note.id %>_tags").empty();
  $("#tags_<%= @note.id %>").show();
}
